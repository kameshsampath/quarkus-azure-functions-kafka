{
  "scriptFile": "../${project.build.finalName}.jar",
  "entryPoint": "dev.kameshs.azure.functions.kafka.runtime.KafkaTriggerFunction.run",
  "bindings": [
    {
      "type": "kafkaTrigger",
      "direction": "in",
      "name": "kafkaTrigger",
      "topic": "my-topic",
      "consumerGroup": "functions",
      "brokerList": "my-cluster-kafka-bootstrap.kafka:9092"
    }
  ]
}